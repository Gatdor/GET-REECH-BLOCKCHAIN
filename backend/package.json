{
  "name": "laravel-laravel",
  "description": "GET-REECH Backend",
  "require": {
    "php": "^8.0",
    "fruitcake/laravel-cors": "^3.0",
    "cloutier/php-ipfs-api": "^0.0.6",
    "laravel/framework": "^8.83",
    "nesbot/carbon": "^2.62.1",
    "supabase/supabase-php": "^0.0.3",
    "laravel/tinker": "^2.10",
    "laravel/sanctum": "^2.15",
    "doctrine/dbal": "^2.13",
    "symfony/css-selector": "^6.0",
    "tijsverkoyen/css-to-inline-styles": "^2.2"
  },
  "require-dev": {
    "fakerphp/faker": "^1.9.1",
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "scripts": {
    "post-autoload-dump": "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump && @php artisan package:discover --ansi",
    "dev": "npm run development",
    "development": "mix",
    "watch": "mix watch",
    "watch-poll": "mix watch -- --watch-options-poll=1000",
    "hot": "mix watch --hot",
    "prod": "npm run production",
    "production": "mix --production"
  },
  "private": true,
  "devDependencies": {
    "axios": "^0.21",
    "laravel-mix": "^6.0",
    "lodash": "^4.17.19",
    "postcss": "^8.1.14"
  },
  "dependencies": {
    "express": "^5.1.0",
    "fabric-ca-client": "^2.2.20",
    "fabric-network": "^2.2.20",
    "fslightbox-react": "^2.0.0",
    "react-qr-code": "^2.0.18",
    "react-to-print": "^3.2.0"
  }
}
